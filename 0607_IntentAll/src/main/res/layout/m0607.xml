<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Gray"
    android:orientation="vertical"
    android:padding="@dimen/Margins">

    <Button
        android:id="@+id/m0000_b0500"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/Margins"
        android:background="@android:drawable/btn_default"
        android:contentDescription="@string/m0000_b0500"
        android:gravity="center_vertical|start"
        android:text="@string/m0000_b0500"
        android:textAllCaps="false"
        android:textSize="@dimen/fontsize"
        tools:ignore="TextContrastCheck" />

    <Button
        android:id="@+id/m0000_b0501"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/Margins"
        android:background="@android:drawable/btn_default"
        android:contentDescription="@string/m0000_b0501"
        android:gravity="center_vertical|start"
        android:text="@string/m0000_b0501"
        android:textAllCaps="false"
        android:textSize="@dimen/fontsize"
        tools:ignore="TextContrastCheck" />

    <Button
        android:id="@+id/m0000_b0502"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/Margins"
        android:background="@android:drawable/btn_default"
        android:contentDescription="@string/m0000_b0502"
        android:gravity="center_vertical|start"
        android:text="@string/m0000_b0502"
        android:textAllCaps="false"
        android:textSize="@dimen/fontsize"
        tools:ignore="TextContrastCheck" />

    <Button
        android:id="@+id/m0000_b0503"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/Margins"
        android:background="@android:drawable/btn_default"
        android:contentDescription="@string/m0000_b0504"
        android:gravity="fill_vertical|start"
        android:text="@string/m0000_b0504"
        android:textAllCaps="false"
        android:textSize="@dimen/fontsize"
        tools:ignore="TextContrastCheck" />

    <Button
        android:id="@+id/m0000_b0505"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/Margins"
        android:background="@android:drawable/btn_default"
        android:contentDescription="@string/m0000_b0505"
        android:gravity="center_vertical|start"
        android:text="@string/m0000_b0505"
        android:textAllCaps="false"
        android:textSize="@dimen/fontsize"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/myname"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/pa01"
        android:gravity="end"
        android:text="@string/myname"
        android:textColor="@color/Yellow"
        android:textSize="@dimen/fontsize"
        tools:ignore="TextContrastCheck" />
</LinearLayout>